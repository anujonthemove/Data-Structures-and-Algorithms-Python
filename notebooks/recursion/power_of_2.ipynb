    "def power_of_2(n):\n",
    "    # 1. write the base case\n",
    "    if n == 0:\n",
    "        return 1\n",
    "\n",
    "    # 2. Reccurence relation\n",
    "    smaller_problem = power_of_2(n - 1)\n",
    "    bigger_problem = 2 * smaller_problem\n",
    "\n",
    "    return bigger_problem"
   ]
  },
