   "source": [
    "def do_sum(arr):\n",
    "\n",
    "    # 1. Base case\n",
    "    if len(arr) == 0:\n",
    "        return 0\n",
    "    if len(arr) == 1:\n",
    "        return arr[0]\n",
    "\n",
    "    smaller_problem = do_sum(arr[1:])\n",
    "    bigger_problem = arr[0] + smaller_problem\n",
    "\n",
    "    return bigger_problem"
   ]
  },
